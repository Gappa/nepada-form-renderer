{
    "name": "nepada/form-renderer",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Latte template based form renderer for Nette forms with full support for Bootstrap 3.",
    "keywords": ["forms", "renderer", "bootstrap", "nette", "latte"],
    "support": {
        "email": "petr@pada.cz",
        "issues": "https://github.com/nepada/form-renderer/issues"
    },
    "authors": [
        {
            "name": "Petr MorÃ¡vek",
            "email": "petr@pada.cz"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "nette/utils": "^2.4@dev",
        "nette/forms": "^2.4.3@dev",
        "nette/application": "^2.4@dev",
        "latte/latte": "^2.4.1@dev"
    },
    "require-dev": {
        "nette/tester": "^1.7",
        "mockery/mockery": "^1.0@alpha",
        "nette/bootstrap": "^2.4@dev || ^3.0@dev",
        "nette/di": "^2.4@dev || ^3.0@dev",

        "nette/component-model": "^2.3@dev || ^3.0@dev",
        "nette/neon": "^2.4@dev || ^3.0@dev",
        "nette/php-generator": "^2.4@dev || ^3.0@dev",
        "nette/http": "^2.4@dev || ^3.0@dev",
        "tracy/tracy": "^2.4@dev || ^3.0@dev"
    },
    "suggest": {
        "nette/di": "for integration with Nette DI container"
    },
    "autoload": {
        "psr-4": {
            "Nepada\\Bridges\\": "src/Bridges/",
            "Nepada\\FormRenderer\\": "src/FormRenderer/"
        },
        "classmap": [
            "src/FormRenderer/exceptions.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
